{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include" : ["src/images.js", "src/wixmedia.js"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "lenient": false
    },
    "templates": {
        "migrateExamples": false,
        "snippetsDir" : "jsdoc/examples",
        "apiTitle" : "WixMP JavaScript API",
        "fullSignatures": false,
        "readme" : "README.md",
        "appendices" : false,
        "showMixins" : false
    }
}