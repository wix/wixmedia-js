{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include" : ["src/images.js", "src/wixmedia.js", "src/node/upload.js", "src/node/authconfig.js", "src/node/AuthClient.js"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "lenient": false
    },
    "templates": {
        "migrateExamples": false,
        "snippetsDir" : "jsdoc/examples",
        "apiTitle" : "WixMP JavaScript API",
        "fullSignatures": false,
        "readme" : "README.md",
        "appendices" : false,
        "showMixins" : false,
        "filters" : {
            "components" : {
                "title" : "Supported Runtimes",
                "sectionTitle" : "Supported Runtimes",
                "list" : {
                    "nodejs" : "Node.js",
                    "browser" : "Browser"
                }
            }
        }
    }
}